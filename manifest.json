{
    "manifest_version": 2,
    "name": "CleanTwitter",
    "description": "CleanTwitter",
    "version": "1.1.0",
    "browser_action": {
        "default_icon": "icons/icon_black.png"
    },
    "icons": {
        "128": "icons/icon_black.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://twitter.com/*",
        "*://api.twitter.com/2/timeline/conversation/*",
        "*://api.twitter.com/2/guide*"
    ]
}